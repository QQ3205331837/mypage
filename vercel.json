{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/image/(.*)",
      "dest": "/static/image/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "FLASK_ENV": "production",
    "PYTHON_VERSION": "3.9",
    "SERVERLESS": "1"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
}